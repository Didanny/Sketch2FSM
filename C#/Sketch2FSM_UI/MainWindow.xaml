<Window x:Class="Sketch2FSM_UI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Sketch2FSM_UI"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="600">
    <StackPanel>
        <Grid>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="*"></ColumnDefinition>
                <ColumnDefinition Width="*"></ColumnDefinition>
                <ColumnDefinition Width="*"></ColumnDefinition>
            </Grid.ColumnDefinitions>
            <Label Grid.Column="1" >Sketch2FSM</Label>
        </Grid>
        <Label Padding="50 0">Drag and drop your image below</Label>
        <StackPanel Height="100" Background="#ebeef4" Margin="50 10"
                    Name="ImagePanel" Drop="ImagePanel_Drop" AllowDrop="true">
            
        </StackPanel>
        <Label Name="TestLabel"></Label>
        <StackPanel Name="sp">
        </StackPanel>
    </StackPanel>
</Window>
